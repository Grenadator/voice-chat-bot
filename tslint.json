{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "arrow-parens": false,
    "class-name": false,
    "interface-name": false,
    "no-console": [
      true,
      "debug",
      "info",
      "time",
      "timeEnd",
      "trace"
    ],
    "quotemark": [
      true,
      "single"
    ],
    "trailing-comma": false
  },
  "rulesDirectory": []
}